{
	"author": "WMXPY",
	"bin": {
		"ghoti": "bin/ghoti",
		"g5": "bin/g5"
	},
	"bugs": {
		"url": "https://github.com/WMXPY/Ghoti-CLI/issues"
	},
	"directories": {
		"doc": "doc"
	},
	"dependencies": {
		"bkc": "^3.2.0",
		"commander": "^2.19.0",
		"module-alias": "^2.2.0"
	},
	"description": ":fish: A CLI tool that helps you start developing your new project within 60 seconds",
	"devDependencies": {
		"@sudoo/triforce": "^3.9.1",
		"@types/module-alias": "^2.0.0",
		"@types/node": "^11.11.3",
		"livescript": "1.6.0"
	},
	"devEngines": {
		"npm": ">=4.x",
		"node": ">=7.x"
	},
	"homepage": "https://github.com/WMXPY/Ghoti-CLI#readme",
	"keywords": [
		"cli",
		"ghoti",
		"react",
		"templates",
		"vue"
	],
	"license": "MIT",
	"main": "bin/import",
	"name": "ghoti-cli",
	"nyc": {
		"extension": [
			".ts",
			".ls"
		],
		"include": [
			"src/**/*.ts",
			"src/**/*.ls",
			"src/*.ls"
		],
		"instrument": true,
		"reporter": [
			"json",
			"text-summary"
		],
		"report-dir": "coverage",
		"require": [
			"./test/tshook.js",
			"livescript"
		],
		"sourceMap": true
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/WMXPY/Ghoti-CLI.git"
	},
	"scripts": {
		"test": "mocha",
		"build": "make",
		"tsTest": "tsc --p ./test/tsconfig.json",
		"cov": "codecov -f ./coverage/coverage-final.json -t [key]"
	},
	"version": "4.1.7"
}
