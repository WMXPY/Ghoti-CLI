{
    "compileOnSave": false,
    "compilerOptions": {
        "inlineSourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "outDir": "../../dist/sudo",
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "lib": [
            "es2016"
        ]
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": [
        "./**/*.d.ts"
    ]
}